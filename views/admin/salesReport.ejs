<!-- Template CSS -->
<link href="../adminHomeAsset/css/main.css" rel="stylesheet" type="text/css" />
<link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.13.1/css/jquery.dataTables.min.css">

<%- include('../partials/adminSideBar') %>
    <main class="main-wrap">
        <section class="content-main">
            <div class="mt-5" style="margin-top: 60px;">
                <h2 class="align-items-center">Sales Report</h2>
                <!-- <div class="border d-flex justify-content-right">
                    <form  method="post" action="/sales-report-date">

                        <input type="date"  class="btn float-right" id="start-date" placeholder="Start date">

                        <input type="date" class="btn" id="end-date" placeholder="End date">

                        <button type="submit" class="btn ">Search</button>

                    </form>
                </div> -->
                

                    <form action="/admin/sales-report" method="get">
                        <div class="row border ">
                    <div class="form-outline col-lg-3  mb-0 mt-3 mb-3">
                        <!-- <label class="form-label" for="form3Example1c">Start Date</label> -->
                        <input type="date" id="startdate" class="form-control" name="start" required />
                    </div>
                    <div class="form-outline col-lg-3  mb-0 mt-3">
                        <!-- <label class="form-label" for="form3Example1c">End Date</label> -->
                        <input type="date" id="enddate" class="form-control" name="end" required />
                    </div>
                    <div class="form-outline col-2 ml-0 mt-3">

                        <button class="btn" type="submit" >Search</button>

                    </div>
                </div>
                </form>

                

                <div class="mt-5" style="margin-top: 60px;">


                    <table class="table mt-4 display" id="productsTable">
                        <thead>
                            <tr>
                                <!-- <th scope="col">Product Image</th> -->
                                <th scope="col">Order ID</th>
                                <th scope="col">Name</th>
                                <th scope="col">Order Date</th>

                                <th scope="col">Payment Methode</th>
                                <th scope="col">Total amount</th>

                                <!-- <th scope="col">User price</th> -->

                            </tr>
                        </thead>
                        <tbody>
                            <% for (var i=0; i < salesReport.length; i++) { %>
                                <tr>

                                    <td>
                                        <%= salesReport[i]._id%>
                                    </td>


                                    <td>
                                        <%= salesReport[i].deliveryDetails.name%>
                                    </td>
                                    <td>
                                        <%= salesReport[i].date%>
                                    </td>
                                    <td>
                                        <%= salesReport[i].paymentMethod%>
                                    </td>
                                    <td>
                                        <%= salesReport[i].total%>
                                    </td>

                                    <!-- <td>
                                            <a href="/admin/single-order/<%=salesReport[i]._id%>"class="btn btn-md rounded font-sm">view</a>
                                            
                                        </td> -->



                                </tr>
                                <% } %>


                        </tbody>
                    </table>
                </div>


        </section> <!-- content-main end// -->

    </main>



    <script src="../adminHomeAsset/js/vendors/jquery-3.6.0.min.js"></script>
    <script src="../adminHomeAsset/js/vendors/bootstrap.bundle.min.js"></script>
    <script src="../adminHomeAsset/js/vendors/select2.min.js"></script>
    <script src="../adminHomeAsset/js/vendors/perfect-scrollbar.js"></script>
    <script src="../adminHomeAsset/js/vendors/jquery.fullscreen.min.js"></script>
    <!-- Main Script -->
    <script src="../adminHomeAsset/js/main.js" type="text/javascript"></script>



    <script type="text/javascript" charset="utf8" src="https://code.jquery.com/jquery-3.5.1.js"></script>
    <script type="text/javascript" charset="utf8"
        src="https://cdn.datatables.net/1.13.1/js/jquery.dataTables.min.js"></script>
        <script src="https://cdn.datatables.net/buttons/2.3.6/js/dataTables.buttons.min.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/pdfmake.min.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/vfs_fonts.js"></script>
        <script src="https://cdn.datatables.net/buttons/2.3.6/js/buttons.html5.min.js"></script>
        <script src="https://cdn.datatables.net/buttons/2.3.6/js/buttons.print.min.js"></script>
    <script>

        $(function () {
            $('#productsTable').DataTable( {
        dom: 'Bfrtip',
        buttons: [
            'copy', 'csv', 'excel', 'pdf', 'print'
        ]
    });
        });

    </script>

    </body>


